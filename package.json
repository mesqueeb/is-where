{
  "name": "is-where",
  "sideEffects": false,
  "type": "module",
  "version": "2.0.1",
  "description": "JS environment check functions like `isWebkit() isSafari() isBrowser() isNode()` etc. A simple & small integration.",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "tsc --noEmit && eslint ./src",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && del dist && npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/is-where.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "envchecker",
    "check-environment",
    "check-browser",
    "is-browser",
    "is-node",
    "is-deno",
    "is-safari",
    "is-chrome",
    "is-webkit",
    "is-edge",
    "is-firefox",
    "is-opera",
    "is-ie",
    "is-ssr",
    "is-server",
    "browser-checking",
    "browser-checker",
    "browser-check",
    "environment-checking",
    "environment-checker",
    "environment-check"
  ],
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/is-where/issues"
  },
  "homepage": "https://github.com/mesqueeb/is-where#readme",
  "devDependencies": {
    "@cycraft/tsconfig": "^0.1.1",
    "@eslint/js": "^9.1.1",
    "@types/node": "^20.12.8",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.5.3"
  },
  "np": {
    "yarn": false,
    "branch": "production"
  }
}
