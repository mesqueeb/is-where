{
  "name": "is-where",
  "version": "2.0.4",
  "description": "JS environment check functions like `isWebkit() isSafari() isBrowser() isNode()` etc. A simple & small integration.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "tsc --noEmit && eslint ./src",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && npm run build && np"
  },
  "devDependencies": {
    "@cycraft/eslint": "^0.3.0",
    "@cycraft/tsconfig": "^0.1.2",
    "del-cli": "^5.1.0",
    "np": "^10.0.5",
    "vitest": "^1.6.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "envchecker",
    "check-environment",
    "check-browser",
    "is-browser",
    "is-node",
    "is-deno",
    "is-safari",
    "is-chrome",
    "is-webkit",
    "is-edge",
    "is-firefox",
    "is-opera",
    "is-ie",
    "is-ssr",
    "is-server",
    "browser-checking",
    "browser-checker",
    "browser-check",
    "environment-checking",
    "environment-checker",
    "environment-check"
  ],
  "author": "Luca Ban - Mesqueeb (https://cycraft.co)",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/is-where.git"
  },
  "homepage": "https://github.com/mesqueeb/is-where#readme",
  "bugs": "https://github.com/mesqueeb/is-where/issues"
}
